section.catalogThread(onclick='expandCatalogOp(event)')
  .catalogCellContent
    header.catalogCellHeader
      a.linkThumb
      span.labelSubject
    .catalogMessageWrap
      p.divMessage
  footer.catalogThreadDetails
    span.labelReplies.fa.fa-comment-o-after
    span.labelImages.fa.fa-picture-o-after
    span.labelPage

    span.lockIndicator.fa.fa-lock(title=l.guiLockLabel)
    span.pinIndicator.fa.fa-thumb-tack(title=l.guiPinLabel)
    span.cyclicIndicator.fa.fa-refresh(title=l.guiCyclicLabel)
